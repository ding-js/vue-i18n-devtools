{
  "name": "vue-i18n-devtools",
  "version": "0.0.0",
  "scripts": {
    "webpack:watch": "webpack --config ./shells/webextension/webpack.config.js --watch",
    "copy:watch": "node ./shells/webextension/copy-watch.js",
    "dev": "concurrently -k -r \"npm run webpack:watch\" \"npm run copy:watch\""
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chokidar": "^2.0.4",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.11.0",
    "fs-extra": "^7.0.1",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ding-js/vue-i18n-devtools.git"
  }
}
